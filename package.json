{
  "name": "surya-vite-react-chakra",
  "version": "0.2.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/sync-version.mjs",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "validate": "yarn typecheck && yarn lint && yarn test",
    "unused:exports": "eslint . --no-eslintrc --ext ts,tsx",
    "unused:ts-prune": "ts-prune",
    "unused:knip": "knip --production --strict",
    "preview": "vite preview",
    "version:sync": "node scripts/sync-version.mjs",
    "version:bump:patch": "yarn version --patch --no-git-tag-version && node scripts/sync-version.mjs"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "framer-motion": "^11.3.24",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "chrome-types": "^0.1.282",
    "eslint": "^9.33.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^25.0.0",
    "knip": "^5.33.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "engineStrict": true
}
